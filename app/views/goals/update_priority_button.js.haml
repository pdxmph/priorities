$("#priority-button-#{id}").html("#{j render(:partial => 'goals/priority_button', :locals => {:list => list, :goal => goal})}");
$("#td_priority_#{id}").attr("data-value", "#{priority}");
$("#td_priority_#{id}").parent('tr').attr("data-priority", "#{priority}");
$("#goal_health_#{id}").html("#{j render(:partial => 'goals/health_field', :locals => {:goal => goal})}");
$("#goal_health_#{id}").attr("data-value", "#{goal.health}");
$("#td_goal_burden_#{id}").html("#{j render(:partial => 'goals/burden_field', :locals => {:goal => goal, :list => list})}");
$("#td_goal_burden_#{id}").attr("data-value", "#{goal.burden}");
